<Application
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    x:Class="DocumentGenerator.UI.App"
    xmlns:local="using:DocumentGenerator.UI"
    RequestedThemeVariant="Light">

    <Application.DataTemplates>
        <local:ViewLocator />
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme>
        </FluentTheme>
    </Application.Styles>

    <Application.Resources>
        <FontFamily
            x:Key="FuturaNewFontFamily">
            avares://DocumentGenerator.UI/Assets/Fonts/FuturaNew#Futura New
        </FontFamily>
    </Application.Resources>
</Application>